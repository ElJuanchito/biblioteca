		biblioteca.crearEstudiante(nombre, apellidos, 20, "sistemas", "activo", "12345");	
		
		biblioteca.crearEstudiante(nombre, apellidos, 20, "sistemas", "activo", "12345");

		biblioteca.crearEstudiante(nombre, apellidos, 20, "sistemas", "activo", "12345");

		biblioteca.crearEstudiante(nombre, apellidos, 20, "sistemas", "activo", "12345");

		biblioteca.crearEstudiante(nombre, apellidos, 20, "sistemas", "activo", "12345");

		System.out.println();
		

